{
  "name": "user-svc",
  "version": "0.0.1",
  "scripts": {
    "dev": "clear && nodemon main.ts run service",
    "migrate:up": "npx knex migrate:latest",
    "migrate:down": "npx knex migrate:rollback",
    "start": "node main.ts run service",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "gitHooks": {
    "pre-commit": "yarn lint:fix"
  },
  "_moduleAliases": {
    "@resources": "resources",
    "@helpers": "internal/service/helpers",
    "@data": "internal/data",
    "@": "internal/service"
  },
  "dependencies": {},
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/express": "^4.17.17",
    "@types/knex": "^0.16.1",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "commander": "^10.0.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "winston": "^3.9.0"
  }
}
