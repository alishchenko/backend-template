{
  "name": "user-svc",
  "version": "0.0.1",
  "scripts": {
    "dev": "clear && nodemon main.ts run service",
    "migrate:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n",
    "migrate:up": "npx knex migrate:latest",
    "migrate:down": "npx knex migrate:rollback",
    "typeorm": "typeorm-ts-node-esm ",
    "start": "node main.ts run service",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "generate-docs": "cd docs && npm run build && cd .. && npx openapi-generator-cli generate -i ./docs/web_deploy/openapi.yaml -g typescript-fetch -o ./resources"
  },
  "gitHooks": {
    "pre-commit": "yarn lint:fix"
  },
  "_moduleAliases": {
    "@": "internal/service",
    "@data": "internal/data",
    "@resources": "resources"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "pg": "^8.11.0",
    "typeorm": "^0.3.16",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "yorkie": "^2.0.0"
  }
}
